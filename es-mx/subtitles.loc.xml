	<string key="Helvan_Death_line1" comment="AI_Helvan_the_Navigator.slice" speaker="Helvan the Navigator" rel_version="Quest_MSQ2_Revamp">Una victoria efímera. ¡Pronto serás como yo!</string>
	<string key="Helvan_JumpsDown_line1" comment="AI_Helvan_the_Navigator.slice" speaker="Helvan the Navigator" rel_version="Quest_MSQ2_Revamp">¡Ah del barco, primor!</string>
	<string key="Imhotep_CombatBark_01" comment="AI_Imhotep.slice" speaker="@Imhotep_VitalsName" rel_version="Territory_Brimstone_Sands">¡Contempla el poder de los Antiguos! ¡Siente su ira!</string>
	<string key="Imhotep_CombatBark_02" comment="AI_Imhotep.slice" speaker="@Imhotep_VitalsName" rel_version="Territory_Brimstone_Sands">¡Soy el Protector de los Misterios! ¡El Guardián de los Secretos!</string>
	<string key="Imhotep_CombatBark_03" comment="AI_Imhotep.slice" speaker="@Imhotep_VitalsName" rel_version="Territory_Brimstone_Sands">¡Ni siquiera tú puedes resistir ante los Antiguos! ¡Deja de enfrentarme!</string>
	<string key="Imhotep_CombatBark_04" comment="AI_Imhotep.slice" speaker="@Imhotep_VitalsName" rel_version="Territory_Brimstone_Sands">¡Acaba con esta batalla inútil! ¡Únete a mí! ¡Puedes ayudarme a calmar a los dioses!</string>
	<string key="Imhotep_CombatBark_05" comment="AI_Imhotep.slice" speaker="@Imhotep_VitalsName" rel_version="Territory_Brimstone_Sands">¡Aeternum les pertenece a los Antiguos! ¡Deja que lo conquisten! ¡No fue más que una maldición sobre nuestro pueblo!</string>
